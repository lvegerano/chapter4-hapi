.panel.panel-default
    .panel-heading
        h3.panel-title Most Popular
    .panel-body
        each popular in sidebar.popular
            .col-md-4.text-center(style="padding-bottom: .5em;")
                a.img-thumbnail(href="/images/#{popular.uniqueId}")
                    img(src="/public/upload/#{popular.filename}", alt="#{popular.filename}", style="width: 75px; height: 75px;")
